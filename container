docker run \
-u nodejs \
-w /home/nodejs/projects/userprofiles/ \
--add-host=db01.domain.local:192.168.99.100 \
-p 8081:8081 \
-v /s:/s \
-v /s/node:/home/nodejs/projects \
--env-file /s/node/env.list \
--name node \
-ti jpoot/node_oracledb:2.0 /bin/bash

